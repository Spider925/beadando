{% extends 'layout' %}

{% block content %}
<div class="col-md-12 col-sm-6">
    <div class="panel panel-primary">
      <div class="panel-heading">
        <h3 class="panel-title">Feltöltött képeid</h3>
      </div>
      <div class="panel-body" align="center">
       {% for picture in pictures %}
           <a href="/picture/{{ picture.name }}" >
            <img border="0" src="/images/{{ picture.name }}.png" class="indexpictures">
          </a>
       {% endfor %}
        </div>
    </div>
</div>

{% endblock %}